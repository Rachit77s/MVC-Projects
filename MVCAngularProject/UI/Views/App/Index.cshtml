
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <base href="/">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/x-icon" href="favicon.ico">
    <title>@ViewBag.Title</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script>
        @*var user={
            userId: '@currentUser.UserId',
            name: '@currentUser.Name',
            username: '@currentUser.Username',
            contactNo: '@currentUser.ContactNo',
            token:'@currentUser.Token'
        }*@
    </script>
</head>
<body>
        <app-root></app-root>
        <script type="text/javascript" src="~/scripts/client/inline.bundle.js"></script>
        <script type="text/javascript" src="~/scripts/client/polyfills.bundle.js"></script>
        <script type="text/javascript" src="~/scripts/client/styles.bundle.js"></script>
        <script type="text/javascript" src="~/scripts/client/scripts.bundle.js"></script>
        <script type="text/javascript" src="~/scripts/client/vendor.bundle.js"></script>
        <script type="text/javascript" src="~/scripts/client/main.bundle.js"></script>

</body>
</html>
